  },
  "homepage": "https://github.com/AdrianoSantana/clean-architecture-typescript#readme",
  "dependencies": {
    "@types/express": "^4.17.13",
    "bcrypt": "^5.0.1",
    "express": "^4.17.2",
    "mongodb": "^4.3.1",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^2.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^27.4.0",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^17.0.8",
    "@types/supertest": "^2.0.11",
